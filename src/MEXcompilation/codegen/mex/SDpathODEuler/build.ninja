# CompilerName=gcc
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/glnxa64
matlabdir = /home/andrew/matlab
startdir = /home/andrew/Dropbox/PathFinderInf/src/MEXcompilation

# Toolchain information
cc = /usr/bin/gcc
cxx = g++
linker = /usr/bin/gcc
linkerxx = g++

cflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=SDpathODEuler_mex -c 

cxxflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -std=c++11 -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=SDpathODEuler_mex -c 

ldflags =  -pthread -Wl,--no-undefined -Wl,-rpath-link,$matlabdir/bin/glnxa64 -shared  -L"$matlabdir/bin/glnxa64" -lmx -lmex -lmat -lm -lstdc++  -o SDpathODEuler_mex.mexa64 -lemlrt -lcovrt -lut -lmwmathutil  -Wl,--version-script,SDpathODEuler_mex.map

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $ldflags

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/SDpathODEuler_data.o : cc $root/SDpathODEuler_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/SDpathODEuler_initialize.o : cc $root/SDpathODEuler_initialize.c
build $builddir/SDpathODEuler_terminate.o : cc $root/SDpathODEuler_terminate.c
build $builddir/SDpathODEuler.o : cc $root/SDpathODEuler.c
build $builddir/mod.o : cc $root/mod.c
build $builddir/angle.o : cc $root/angle.c
build $builddir/abs.o : cc $root/abs.c
build $builddir/indexShapeCheck.o : cc $root/indexShapeCheck.c
build $builddir/beyondNoReturn.o : cc $root/beyondNoReturn.c
build $builddir/_coder_SDpathODEuler_api.o : cc $root/interface/_coder_SDpathODEuler_api.c
build $builddir/_coder_SDpathODEuler_mex.o : cc $root/interface/_coder_SDpathODEuler_mex.c
build $builddir/eml_int_forloop_overflow_check.o : cc $root/eml_int_forloop_overflow_check.c
build $builddir/exp.o : cc $root/exp.c
build $builddir/SDpathODEuler_emxutil.o : cc $root/SDpathODEuler_emxutil.c
build $builddir/_coder_SDpathODEuler_info.o : cc $root/interface/_coder_SDpathODEuler_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/SDpathODEuler_mex.mexa64 : link $builddir/SDpathODEuler_data.o $builddir/rt_nonfinite.o $builddir/SDpathODEuler_initialize.o $builddir/SDpathODEuler_terminate.o $builddir/SDpathODEuler.o $builddir/mod.o $builddir/angle.o $builddir/abs.o $builddir/indexShapeCheck.o $builddir/beyondNoReturn.o $builddir/_coder_SDpathODEuler_api.o $builddir/_coder_SDpathODEuler_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/exp.o $builddir/SDpathODEuler_emxutil.o $builddir/_coder_SDpathODEuler_info.o $builddir/c_mexapi_version.o 
