# CompilerName=gcc
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/glnxa64
matlabdir = /home/andrew/matlab
startdir = /home/andrew/Dropbox/PathFinderInf/src/MEXcompilation

# Toolchain information
cc = /usr/bin/gcc
cxx = g++
linker = /usr/bin/gcc
linkerxx = g++

cflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=SE_NEwtonCorrection_mex -c 

cxxflags =  -fexceptions -fPIC -fno-omit-frame-pointer -pthread -std=c++11 -D_GNU_SOURCE -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=SE_NEwtonCorrection_mex -c 

ldflags =  -pthread -Wl,--no-undefined -Wl,-rpath-link,$matlabdir/bin/glnxa64 -shared  -L"$matlabdir/bin/glnxa64" -lmx -lmex -lmat -lm -lstdc++  -o SE_NEwtonCorrection_mex.mexa64 -lemlrt -lcovrt -lut -lmwmathutil  -Wl,--version-script,SE_NEwtonCorrection_mex.map

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $ldflags

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/SE_NEwtonCorrection_data.o : cc $root/SE_NEwtonCorrection_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/SE_NEwtonCorrection_initialize.o : cc $root/SE_NEwtonCorrection_initialize.c
build $builddir/SE_NEwtonCorrection_terminate.o : cc $root/SE_NEwtonCorrection_terminate.c
build $builddir/SE_NEwtonCorrection.o : cc $root/SE_NEwtonCorrection.c
build $builddir/_coder_SE_NEwtonCorrection_api.o : cc $root/interface/_coder_SE_NEwtonCorrection_api.c
build $builddir/_coder_SE_NEwtonCorrection_mex.o : cc $root/interface/_coder_SE_NEwtonCorrection_mex.c
build $builddir/eml_int_forloop_overflow_check.o : cc $root/eml_int_forloop_overflow_check.c
build $builddir/SE_NEwtonCorrection_emxutil.o : cc $root/SE_NEwtonCorrection_emxutil.c
build $builddir/_coder_SE_NEwtonCorrection_info.o : cc $root/interface/_coder_SE_NEwtonCorrection_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/SE_NEwtonCorrection_mex.mexa64 : link $builddir/SE_NEwtonCorrection_data.o $builddir/rt_nonfinite.o $builddir/SE_NEwtonCorrection_initialize.o $builddir/SE_NEwtonCorrection_terminate.o $builddir/SE_NEwtonCorrection.o $builddir/_coder_SE_NEwtonCorrection_api.o $builddir/_coder_SE_NEwtonCorrection_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/SE_NEwtonCorrection_emxutil.o $builddir/_coder_SE_NEwtonCorrection_info.o $builddir/c_mexapi_version.o 
